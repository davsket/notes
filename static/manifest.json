{
    "name": "Notes App",
    "description": "Create and edit your notes locally",
    "version": "1",
    "app": {
        "urls": [
            "*://notes.davsket.me",
            "*://davsket.me/notes"
        ],
        "launch": {
            "web_url": "http://notes.davsket.me"
        }
    },
    "icons": {
        "128": "icon128.png"
    },
    "offline_enabled": true,
    "permissions": [
        "unlimitedStorage",
        "notifications",
        "*://davsket.me/notes"
    ]
}